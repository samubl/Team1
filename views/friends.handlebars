<html>
<head>
	<title>Team1 J. Smith Homepage</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/css/friends.css" rel="stylesheet">
</head>
<body>
	<div class="container">
		<button type="button" onclick="goBack()" id="back">Back</button> 
		<h1 id="friends">Friends</h1>	
		<hr>
		{{#each friends}}
			<img src="{{picture}}" class="img-responsive img-rounded">
			<a href="#"><h3>{{username}}</h3></a>
			<hr>
		{{/each}}
		<ul class="navbar">
			<li><a href="/">Home</a></li>
			<li><a href="{{username}}" class="active">Account</a></li>
			<li><a href='{{username}}/Friends'>Friends</a></li>
			<li><a href="{{username}}/Discover">Discover</a></li>
		</ul>	
	</div>

	<script src="/js/jquery-1.11.0.js"></script>	
	<script src="/js/bootstrap.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script>
		function goBack(){
			window.history.back();
		}
	</script>
</body>
</html>